// begin join section
#section2.section.join-section
  include ../elements/clouds-top

  .container.join-section__container
    .join-section__header
      h2.join-section__title Все грани отдыха
      h3.join-section__subtitle Веселая атмосфера, вежливый персонал, элитный алкоголь, эксклюзивные коктейли от лучших барменов города и, конечно же, "жемчужинки" нашего клуба - самые сексуальные танцовщицы!

    .join-section__body
      .join-section__body-pic
      .join-section-tabs
        ul.join-section-tabs__header
          li.join-section-tabs__tab.active(data-tab="#join-tab-01") Женский стриптиз
          li.join-section-tabs__tab(data-tab="#join-tab-02") Мужской стриптиз
          li.join-section-tabs__tab(data-tab="#join-tab-03") Мальчишник
          li.join-section-tabs__tab(data-tab="#join-tab-04") Девичник
          li.join-section-tabs__tab(data-tab="#join-tab-05") Караоке
          li.join-section-tabs__tab(data-tab="#join-tab-06") Кальян

